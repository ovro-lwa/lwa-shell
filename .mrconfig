# myrepos config file for ovro-lwa Project

[swdeploy]
checkout =
    git clone https://github.com/ovrocaltech/swdeploy.git

[lwa-cfg]
checkout =
    git clone git@github.com:ovro-lwa/lwa-cfg.git

[lwa-pyutils]
checkout =
    git clone git@github.com:ovro-lwa/lwa-pyutils.git

[lwa-controlscripts]
checkout =
    git clone git@github.com:ovro-lwa/lwa-controlscripts.git

[lwa-antpos]
checkout =
    git clone git@github.com:ovro-lwa/lwa-antpos.git

[ARXtesting]
checkout =
    git clone git@github.com:ovro-lwa/ARXtesting.git

[mnc_python]
checkout =
    git clone git@github.com:ovro-lwa/mnc_python.git

[lwa-issues]
checkout =
    git clone git@github.com:ovro-lwa/lwa-issues.git

[caltech-lwa]
checkout =
    git clone https://github.com/realtimeradio/caltech-lwa
    cd caltech-lwa
    git submodule init control_sw/libs/casperfpga
    git submodule update

[dsa110-pyutils]
checkout =
    git clone https://github.com/dsa110/dsa110-pyutils.git
